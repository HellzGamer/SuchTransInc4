<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Such Trans Logistics</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



