<body class="Admin">
    <div class="navbar navbar-custom navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("SuchTrans Logistcs", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <div class="nav-side-menu">
        <div class="brand">Such Trans Logistics</div>
        <i class="fa fa-bars fa-2x toggle-btn" data-toggle="collapse" data-target="#menu-content"></i>

        <div class="menu-list">

            <ul id="menu-content" class="menu-content collapse out">
                <li>
                    <a href="#">
                        <i class="fa fa-dashboard fa-lg"></i> Administrator
                    </a>
                </li>
                <li data-toggle="collapse" data-target="#products" class="collapsed active">
                    <a href="#"><i class="fa fa-users fa-lg"></i> Employee Management <span class="arrow"></span></a>
                </li>
                <ul class="sub-menu collapse" id="products">
                    <li><a href=@Url.Action("AllEmployees","Admin")>View All Employees</a></li>
                    <li><a href=@Url.Action("Register", "Admin")>Register Employee</a></li>
                </ul>

                <li data-toggle="collapse" data-target="#service" class="collapsed">
                    <a href="#"><i class="fa fa-globe fa-lg"></i> Delivery Management <span class="arrow"></span></a>
                </li>
                <ul class="sub-menu collapse" id="service">
                    <li><a href=@Url.Action("AllDeliveries", "Admin")>View All Deliveries</a></li>
                    <li><a href=@Url.Action("DeliveryJobs", "Admin")>View All Delivery Jobs</a></li>
                    <li><a href=@Url.Action("JobQueue", "Admin")>View Job Queue</a></li>
                    <li><a href=@Url.Action("ScheduleJobs", "Admin")>Schedule Delivery Jobs</a></li>
                    <li><a href=@Url.Action("PrioritizeDelivery","Prioritization")>Prioritize Delivery</a></li>
                    <li><a href=@Url.Action("SearchDelivery", "Driver")>Notify System of Pick Up/Drop Off</a></li>
                </ul>

                <li data-toggle="collapse" data-target="#new" class="collapsed">
                    <a href="#"><i class="fa fa-globe fa-lg"></i> Driver and Fleet Management <span class="arrow"></span></a>
                </li>
                <ul class="sub-menu collapse" id="new">
                    <li><a href=@Url.Action("Index", "Fleet")>View Fleet</a></li>
                    <li><a href=@Url.Action("Index", "TruckDriver")>View Drivers</a></li>
                    <li><a href=@Url.Action("SearchTruck", "Breakdown")>Manage Breakdowns</a></li>
                    @*<li>New New 3</li>*@
                </ul>

                <li data-toggle="collapse" data-target="#web" class="collapsed">
                    <a href="#"><i class="fa fa-globe fa-lg"></i> Website Management <span class="arrow"></span></a>
                </li>
                <ul class="sub-menu collapse" id="web">
                    <li><a href=@Url.Action("Index", "StaticData")>Contact page Information</a></li>
                    @*<li><a href=@Url.Action("About", "StaticData")>Change About Page Information</a></li>*@
                </ul>

                <li>
                    <a href=@Url.Action("YourProfile", "Admin")>
                        <i class="fa fa-user fa-lg"></i> Profile
                    </a>
                </li>
            </ul>
        </div>

    </div>
    <div class="container body-content" style="padding-left:300px; padding-top:40px;">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>



